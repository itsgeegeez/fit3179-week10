    "transform": [
        { "filter": "datum.dun == dun_selection" },
        {
            "fold": [
                "votertype_regular",
                "votertype_early_army",
                "votertype_early_police",
                "votertype_postal_overseas"
            ],
            "as": ["votertype", "value"]
        },
        {
            "calculate": "indexof(datum.votertype, 'regular') > -1 ? 'Regular' : indexof(datum.votertype, 'early_army') > -1 ? 'Early Army' : indexof(datum.votertype, 'early_police') > -1 ? 'Early Police' : 'Postal Overseas'",
            "as": "votertype"
        }
    ],

    "title": {
        "text": "Constituency Voters by Type",
        "font": "Arial",
        "subtitle": "Highlight a voter type to see the distribution of voters by voter type"
    },

    "layer": [
        {
            "mark": {
                "type": "bar",
                "width": 48
            },
            "params": [
                {
                    "name": "type_selection",
                    "select": {
                        "type": "point",
                        "fields": ["votertype"]
                    },
                    "bind": "legend"
                }
            ],
            "encoding": {
                "x": {
                    "field": "votertype",
                    "type": "ordinal",
                    "title": "Voter Type",
                    "axis": { "labelAngle": -45 }
                },
                "y": {
                    "field": "value",
                    "type": "quantitative",
                    "title": "Number of Voters"
                },
                "color": {
                    "field": "votertype",
                    "title": "Voter Type",
                    "type": "nominal"
                },
                "opacity": {
                    "condition": { "param": "type_selection", "value": 1 },
                    "value": 0.2
                },
                "tooltip": [
                    { "field": "dun", "type": "nominal", "title": "Constituency" },
                    { "field": "votertype", "type": "nominal", "title": "Voter Type" },
                    { "field": "value", "type": "quantitative", "title": "Number of Voters", "format": ",.0f" }
                ]
            }
        }
    ],
    "config": {
        "view": { "stroke": null },
        "axis": { "grid": false }
    }
}
